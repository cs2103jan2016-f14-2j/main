                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </AnchorPane>
                        </content>
                      </Tab>
                      <Tab fx:id="eventsTab" text="Events">
                        <content>
                          <AnchorPane fx:id="eventsContent" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </AnchorPane>
                        </content>
                      </Tab>
                        <Tab fx:id="deadlinesTab" text="Deadlines">
                          <content>
                            <AnchorPane fx:id="deadlinesContent" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </AnchorPane>
                          </content>
                        </Tab>
                        <Tab fx:id="todoTab" text="To-do">
                           <content>
                              <AnchorPane fx:id="todoContent" minHeight="0.0" minWidth="0.0" prefHeight="239.0" prefWidth="800.0">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </AnchorPane>
                           </content>
                        </Tab>
                    </tabs>
                  </TabPane>
                  <VBox maxHeight="23.0" VBox.vgrow="ALWAYS">
                     <children>
                        <HBox>
                           <children>
                              <Region HBox.hgrow="ALWAYS" />
                              <Label fx:id="messagePrompt" contentDisplay="CENTER" textAlignment="CENTER" />
                              <Region HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <HBox spacing="10.0">
                     <children>
                        <TextField fx:id="commandBox" onAction="#enterTriggered" prefHeight="25.0" prefWidth="715.0" promptText="enter command here" HBox.hgrow="ALWAYS" />
                        <Button fx:id="enterButton" minWidth="-Infinity" mnemonicParsing="false" onAction="#enterTriggered" text="Enter" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <VBox fx:id="overlay">
         <children>
            <Region VBox.vgrow="ALWAYS" />
            <HBox>
               <children>
                  <Region HBox.hgrow="ALWAYS" />
                  <AnchorPane fx:id="popupLayer">
                     <children>
                        <AnchorPane fx:id="helpBox" layoutX="10.0" layoutY="10.0" style="-fx-background-color: ivory; -fx-border-color: grey; -fx-border-radius: 10; -fx-padding: 10; -fx-background-radius: 10;">
                           <children>
                              <VBox layoutX="10.0" layoutY="15.0" spacing="10.0">
                                 <children>
                                    <HBox>
                                       <children>
                                          <Label text="Help Sheet" />
                                          <Region HBox.hgrow="ALWAYS" />
                                          <ImageView fx:id="helpCloseButton" fitHeight="17.0" fitWidth="17.0" onMouseClicked="#searchCloseButtonAction" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@../../../../closeBtn.png" />
                                             </image>
                                          </ImageView>
                                       </children>
                                    </HBox>
                                    <ScrollPane hbarPolicy="NEVER">
                                       <content>
                                          <AnchorPane prefHeight="310.0" prefWidth="584.0">
                                             <children>
                                                <Text fx:id="helpContent" layoutX="14.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                                             </children>
                                          </AnchorPane>
                                       </content>
                                    </ScrollPane>
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                        <AnchorPane fx:id="searchBox" style="-fx-background-color: ivory; -fx-border-color: grey; -fx-border-radius: 10; -fx-padding: 10; -fx-background-radius: 10;">
                           <children>
                              <VBox layoutX="10.0" layoutY="15.0" spacing="10.0">
                                 <children>
                                    <HBox>
                                       <children>
                                          <Label text="Search Results" />
                                          <Region HBox.hgrow="ALWAYS" />
                                          <ImageView fx:id="searchCloseButton" fitHeight="17.0" fitWidth="17.0" onMouseClicked="#searchCloseButtonAction" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@../../../../closeBtn.png" />
                                             </image>
                                          </ImageView>
                                       </children>
                                    </HBox>
                                    <ListView fx:id="searchList" editable="true" prefHeight="400.0" prefWidth="600.0" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                     </children></AnchorPane>
                  <Region HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <Region VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</StackPane>
